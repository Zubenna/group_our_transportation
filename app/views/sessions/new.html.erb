<h1>Log In</h1>
<%= form_with url: login_path, class: 'login-page' do |f| %>
 <div class='session-login'>
   <%= f.label :username, class: 'login-label' %>
   <%= f.text_field :username, class: 'login-input', placeholder: 'Enter your username' %>
 </div>
 <div class='login-btn'>
   <%= f.submit 'Login', class: 'login' %>
 </div>
<% end %>